<?php 

    $x = $_GET['x'];    // ввод числа пользователем

    $a = 1;     // переменная 1
    $b = 1;     // переменная 2


    if ($x <= 0) {                                                      // вывод на экран сообщения, если пользователь ввел не положительное число 
            echo "Введено некорректное значение. Повторите ввод.";      // ноль, отрицательное число, буквы и символы - некорректное значение
        }

    for ($i = 0; $i < $x; $i++) {

        if ($a > $x) {
                echo "Задуманное число НЕ входит в числовой ряд.";
                break; 
        }
        elseif ($a == $x) {
                echo "Задуманное число входит в числовой ряд.";
                break;
        }
            else {
                $c = $a;
                $a += $b;
                $b = $c;
            }        
    }
 ?>
